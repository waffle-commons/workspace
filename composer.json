{
  "name": "waffle-commons/workspace",
  "description": "Local development and integration testing environment for Waffle framework and components.",
  "type": "project",
  "version": "0.1.0-alpha1",
  "require": {
    "php": "^8.4",
    "waffle-commons/waffle": "1.0.0-dev",
    "waffle-commons/http": "1.0.0-dev",
    "waffle-commons/container": "1.0.0-dev"
  },
  "repositories": [
    {
      "type": "path",
      "canonical": false,
      "url": "../waffle",
      "options": {
        "versions": { "waffle-commons/waffle": "1.0.0-dev" },
        "symlink": true
      }
    },
    {
      "type": "path",
      "canonical": false,
      "url": "../http",
      "options": {
        "versions": { "waffle-commons/http": "1.0.0-dev" },
        "symlink": true
      }
    },
    {
      "type": "path",
      "canonical": false,
      "url": "../container",
      "options": {
        "versions": { "waffle-commons/container": "1.0.0-dev" },
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Workspace\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WorkspaceTests\\": "tests/app/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": false
}